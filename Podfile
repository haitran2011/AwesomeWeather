    pod 'Cuckoo'
    pod 'RxSwift',    '~> 3.0'
    pod 'RxCocoa',    '~> 3.0'
    
  target 'AwesomeWeatherTests' do
      inherit! :search_paths
      # Pods for testing
      pod 'RxBlocking', '~> 3.0'
      pod 'RxTest',     '~> 3.0'
  end

end
